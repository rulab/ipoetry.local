        console.log(ajaxdata);
        url='{{ path('IpoetryBundle_unewsfeedentityajax') }}';
        type='post';
        dataType='json';
        onsuccess=function(json) {
            console.log(json['result']);
            //если 1 то скррываем кнопку репоста, т.к. пользователь владелец стиха
            if (json['result']===1) {
                $('#PoetryRepostbtn').fadeOut();
            } else {
